    <assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2" 
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
      <id>bin</id>
      <formats>
        <format>zip</format>
      </formats>
      <fileSets>
        <fileSet>
	  <directory>${project.basedir}/../resources</directory>
          <outputDirectory>wakeVoter/resources</outputDirectory>
          <includes>
	    <include>**/*</include>
  	</includes>
	<excludes>
	   <exclude>partyChange/*.csv</exclude>
	   <exclude>precincts/voters/current/*</exclude>
	   <exclude>precincts/voters/previous/*</exclude>
	   <exclude>precincts/maps/**/*</exclude>
	</excludes>
        </fileSet>
        <fileSet>
	  <directory>${project.basedir}/scripts/</directory>
          <outputDirectory>wakeVoter/bin</outputDirectory>
          <includes>
	    <include>**/*</include>
  	</includes>
        </fileSet>
        <fileSet>
	  <directory>${project.build.directory}/wakeVoter</directory>
          <outputDirectory>wakeVoter</outputDirectory>
          <includes>
	    <include>**/*</include>
          </includes>
        </fileSet>
      </fileSets>
    </assembly>
